<!DOCTYPE html>
<html lang="en">
<head>
    
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - GezGebze</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
	   <link rel="stylesheet" href="~/css/homeindexcss/owl.carousel.min.css">
<link rel="stylesheet" href="~/css/homeindexcss/owl.theme.default.min.css">
    <link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family=Montserrat:300,300i,400,400i,500,600,700,800%7CBaloo">
	    <link rel="stylesheet" href="~/css/homeindexcss/bootstrap.css" />
    <link rel="stylesheet" href="~/css/homeindexcss/fonts.css">
    <link rel="stylesheet" href="~/css/homeindexcss/style.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/MaterialDesign-Webfont/1.4.57/css/materialdesignicons.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.css">



    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">
                <img src="~/assets/images/logo.png" width="48px" height="48px"> <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">GezGebze</a><img />
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Camiiler">Camiiler</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Cesmeler">Çeşmeler</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Kalevekuleler">Kale</a>
                        </li>
						<li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Hamamlar">Hamamlar</a>
                        </li>
						<li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Dogaturizmi">Doğa Turizmi</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Yemekler">Yöresel Yemekler</a>
                        </li>
						<li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Sehitlik">Gebze Şehitliği</a>
                        </li>
                       
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Admin" asp-action="Login">Yönetici Girişi</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    
       @RenderBody()
    

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2022 - gebzedemo - <a asp-area="" asp-controller="Home" asp-action="Privacy">Gizlilik</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>

    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
	
    <script src="~/js/site.js" asp-append-version="true"></script>
	<script src="~/js/jsindex/core.min.js"></script>
    <script src="~/js/jsindex/script.js"></script>	
    <script src="~/js/jsindex/owl.carousel.min.js"></script>
         

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
